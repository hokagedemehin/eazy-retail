{
  "expo": {
    "name": "easy-retail",
    "slug": "easy-retail",
    "version": "1.0.0",
    "scheme": "easyretail",
    "orientation": "portrait",
    "icon": "./assets/easylogo3.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/easyspalsh2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/easylogo3.png",
        "backgroundColor": "#336353"
      },
      "package": "com.demibk.easyretail"
    },
    "web": {
      "favicon": "./assets/easylogo3.png"
    },
    "extra": {
      "eas": {
        "projectId": "1b05b21e-d2c1-4c55-b697-5ad285b91f45"
      },
      "backendUrl": "https://api.easyretail.app"
    },
    "plugins": ["expo-community-flipper", "expo-image-picker"]
  }
}
